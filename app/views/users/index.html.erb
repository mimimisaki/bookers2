<h2>Users</h2>
<table>
	<thead>
		<tr>
			<th>image</th>
			<th>name</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
			<td><%= attachment_image_tag user, :profile_image, format: 'jpeg', size: "100x100", class: "img-circle pull-left profile-img", fallback: "no_image.jpg" %></td>
			<td><%= user.name %></td>
			<td><%= link_to "show", user_path(user.id) %></td>
		</tr>
		<% end %>
	</tbody>
</table>